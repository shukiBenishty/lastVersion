{
  "name": "isomorphic-react",
  "description": "Sample employee directory app built with React and Firebase",
  "scripts": {
    "buildTestClient": "webpack --debug --watch --config ./webpack.client.config.js",
    "buildTestServer": "webpack --debug --watch --config ./webpack.server.config.js",
    "buildTestFunc": "webpack --debug --watch --config ./webpack.func.client.config.js",
    "buildClient": "webpack --debug --config ./webpack.client.config.js",
    "buildServer": "webpack --debug --config ./webpack.server.config.js",
    "buildLocalServer": "webpack --debug --config ./webpack.localServer.client.config.js",
    "build": "npm run buildClient && npm run buildServer",
    "test": "npm run buildServer && npm run buildLocalServer &&node ./localServer/hook.js",
    "deploy": "npm run build && firebase deploy",
    "serve": "npm run build && firebase serve --only hosting,functions"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.8",
    "express": "^4.16.2",
    "firebase": "3.9.0",
    "firebase-admin": "~5.6.0",
    "firebase-functions": "^0.8.1",
    "intl": "^1.2.5",
    "intl-locales-supported": "^1.0.0",
    "jquery": "^3.2.1",
    "lodash": "4.17.4",
    "material-ui": "0.17.1",
    "popper.js": "^1.12.9",
    "react": "15.5.4",
    "react-date-picker": "^6.6.2",
    "react-dom": "15.5.4",
    "react-redux": "^5.0.6",
    "react-router": "4.1.1",
    "react-router-dom": "4.1.1",
    "react-table": "^6.7.6",
    "react-tap-event-plugin": "^2.0.1",
    "react-widgets": "^4.1.1",
    "redux": "^3.7.2",
    "redux-form": "^7.2.0",
    "redux-form-material-ui": "^4.3.3",
    "redux-form-website-template": "0.0.97",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.19.1",
    "underscore": "^1.8.3",
    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-loader": "^7.1.2",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "isomorphic-style-loader": "^4.0.0",
    "postcss-loader": "^2.0.10",
    "url-loader": "^0.6.2"
